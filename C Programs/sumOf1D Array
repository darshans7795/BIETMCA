#include <stdio.h>
#include <conio.h>

int main() {
    int row, column, search;

    printf("Enter the number of rows & columns: ");
    scanf("%d%d", &row, &column);

    int array[row][column];

    printf("Enter the elements:\n");
    for (int i = 0; i < row; i++) {
        for (int j = 0; j < column; j++) {
            scanf("%d", &array[i][j]);
        }
    }

    printf("Entered elements are:\n");
    for (int i = 0; i < row; i++) {
        for (int j = 0; j < column; j++) {
            printf("[%d] ", array[i][j]);
        }
        printf("\n");
    }

    int sum = 0;
    for (int i = 0; i < row; i++) {
        for (int j = 0; j < column; j++) {
            sum += array[i][j];
        }
    }
    return 0;
    getch();
    }
