
public class Mian {
public static void max(int arr[]) {
	int firstMax = Integer.MIN_VALUE;
	int secondMax = Integer.MIN_VALUE;
	int thirdMax = Integer.MIN_VALUE;
	
	for(int i=0;i<arr.length;i++) {
		if(arr[i]>firstMax) {
			thirdMax = secondMax;
			secondMax = firstMax;
			firstMax = arr[i];
		} else if(arr[i]>secondMax && arr[i]!=firstMax) {
			thirdMax = secondMax;
			secondMax =arr[i];
		}  else if(arr[i]>thirdMax && arr[i]!=firstMax&&arr[i]!=secondMax) {
			secondMax = arr[i];
		}
	}
	
	System.out.println("1Max Of  Element are: "+firstMax);
	System.out.println("2Max Of  Element are: "+ secondMax );
	System.out.println("3Max Of  Element are: "+ thirdMax );
	
	
}

public static void main(String[] args) {
	int[] arr = {10,5,3,90,33,74,8};
	max(arr);
}
}
    
