
public class x {

	
	
	public static void Xylim(int num) {
		int temp = num;
		int last = temp %10;
		temp = temp/10;
		
		int sum = 0;
		while(temp>9) {
			int rem = temp %10;
			sum = sum + rem;
			temp = temp/10;
		}
		
		int first = temp;
		System.out.println("First Digit: "+first);
		System.out.println("Last Digit: "+last);
		
		if(first+last==sum) {
			System.out.println("Its is Xylem number");
		} else {
			System.out.println("its is not Xylem number");
		}
		
	}

	public static void main(String[] args) {
	Xylim(1056);
	}
}
